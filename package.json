  "scripts": {
    "commit": "git-cz",
    "docs:dev": "npm run build:components && vitepress dev docs",
    "docs:dev:watch": "concurrently --group \"npm run build:components:watch\" \"vitepress dev docs\"",
    "docs:build": "npm run build:components && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "create:component": "nx workspace-generator create-or-update-component",
    "update:component": "nx workspace-generator create-or-update-component",
    "build:components": "nx run components:build",
    "build:components:watch": "nx run components:watch",
    "build:tokens": "nx run tokens:build",
    "test": "web-test-runner",
    "test:watch": "web-test-runner --watch",
    "test:coverage": "web-test-runner --coverage",
    "publish:components": "nx run components:publish",
    "publish:tokens": "nx run tokens:publish"
  },
